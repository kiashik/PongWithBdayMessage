//-------gameOver--------------------------------------
void gameOver()
{
  gameIsRunning = false;
  display.fillScreen(BLACK);
  drawCourt();
  if(PLAYER_SCORE>CPU_SCORE)
  {
    display.setCursor(5,4);
    display.setTextColor(WHITE);
    display.setTextSize(2);
    display.print("You Won"); display.display();
  }else
  {
    display.setCursor(5,4);
    display.setTextColor(WHITE);
    display.setTextSize(2);
    display.print("CPU WON"); display.display();
  }

  display.setCursor(20,30);
  display.setTextColor(WHITE);
  display.setTextSize(3);
  display.print(String(CPU_SCORE)); display.display();

  display.setCursor(60,30);
  display.setTextColor(WHITE);
  display.setTextSize(3);
  display.print(String(PLAYER_SCORE)); display.display();
  
  delay(5000);

  while(digitalRead(UP_BUTTON) == HIGH && digitalRead(DOWN_BUTTON) == HIGH)  
  {
    delay(100);
  }
  gameIsRunning = true;
  
  CPU_SCORE = PLAYER_SCORE = 0;
  
  unsigned long start = millis();
 display.fillScreen(BLACK);
 drawCourt();
  while(millis() - start < 2000);
  ball_update = millis();    
  paddle_update = ball_update;
  gameIsRunning = true;
  resetBall=true;
  
  mode=0;         //will return to modePage after gameover
}////////////////////////////////////////////////////////
